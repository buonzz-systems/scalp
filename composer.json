{
    "name": "buonzz/scalp",
    "description": "Command line tool to Analyze and store your Media file's metadata",
    "license": "MIT",
    "keywords": ["getID3", "media files", "video", "image", "content management", "mongodb import"],
    "authors": [
        {
            "name": "Darwin Biler",
            "email": "darwin@buonzz.com"
        }
    ],    
    "require": {
		"php": ">=5.3.28",
        "symfony/console": "2.*",
        "symfony/finder": "2.3.*",
        "psr/log":"*",
        "james-heinrich/getid3": "1.9.*"
    },
	"require-dev": {       
		"phpunit/phpunit": "3.7.*"
    },
	"autoload": {
        "psr-4": {
            "Buonzz\\Scalp\\": "src/"
        }
    },
    "bin": ["bin/scalp"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/neitanod/forceutf8"
        }
    ],
    "minimum-stability": "dev"
}
